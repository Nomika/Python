
liczby = [1, 13, 12, 10, 7, 8, 6, 11, 5, 2]
print(liczby)
print(len(liczby))
print(sum(liczby))
print(min(liczby))
print(max(liczby))
print((sum(liczby))/(len(liczby)))
liczby.append(15)
print(liczby)
liczby.reverse()
print(liczby)
liczby.pop(1)
print(liczby)
liczby.insert(2, "banana")
liczby.insert(4, "orange")
liczby.insert(6, "apple")
print(liczby)
liczby.remove(5)
print(liczby)
print(len(liczby))
print(liczby)
liczby.pop(2)
print(liczby)
liczby.remove(8)
print(liczby)


